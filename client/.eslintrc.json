{
  "env": {
    "browser": true,
    "es2021": true
  },

  "extends": [
   "next/core-web-vitals",
   "next/typescript",
   "eslint:recommended",
   "plugin:@typescript-eslint/recommended"
  
  ],

 "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "warn", // Customize rules here
    "@typescript-eslint/explicit-module-boundary-types": "off"
  }
   
}
